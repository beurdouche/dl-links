{
  "name": "DL-links",
  "version": "1.0",
  "description": "Collect links from a webpage",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Collect links",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}
